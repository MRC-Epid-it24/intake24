  "scripts": {
    "development": "webpack --env NODE_ENV=development",
    "dev": "npm run development",
    "clean": "rimraf dist",
    "build": "webpack --env NODE_ENV=production",
    "production": "npm run clean && npm run build",
    "prod": "npm run production",
    "start": "node dist/server.js",
    "migrate:foods": "npx sequelize db:migrate --options-path sequelize/foods/options.js",
    "migrate:system": "npx sequelize db:migrate --options-path sequelize/system/options.js",
    "lint": "eslint --ext .js,.ts src/ tests/",
    "lint:fix": "npm run lint -- --fix",
    "test:unit": "jest --testMatch '**/tests/unit/**/*.test.ts'",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:integration": "jest ./tests/integration/index.test.ts --runInBand",
    "test:integration:watch": "npm run test:integration -- --watch"
  },
