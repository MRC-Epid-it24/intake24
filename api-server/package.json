{
  "name": "@intake24/api-server",
  "version": "1.0.0",
  "private": true,
  "description": "Intake24 API Server",
  "author": "",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/MRC-Epid-it24/intake24.git",
    "directory": "api-server"
  },
  "main": "src/index.ts",
  "engines": {
    "node": ">=10.20.1"
  },
  "scripts": {
    "development": "cross-env NODE_ENV=development webpack",
    "development:start": "cross-env NODE_ENV=development nodemon --inspect=6060 --watch dist dist/server.js",
    "dev": "npm run development",
    "clean": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack",
    "production": "npm run clean && npm run build",
    "prod": "npm run production",
    "start": "node dist/server.js",
    "migrate:foods":  "npx sequelize db:migrate --options-path sequelize/foods/options.js",
    "migrate:system":  "npx sequelize db:migrate --options-path sequelize/system/options.js",
    "lint": "eslint --ext .js,.ts src/ --quiet --fix",
    "test:unit": "cross-env NODE_ENV=development webpack --inspect=5959 --config webpack.test.config.js",
    "test:unit:run": "mocha -c dist-tests/tests.js",
    "test:integration:api": "mocha --exit --require ts-node/register --require tsconfig-paths/register tests/integration/api/index.test.ts"
  },
  "dependencies": {
    "@duosecurity/duo_web": "^1.0.5",
    "axios": "^0.20.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "express-validator": "^6.6.1",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "liblevenshtein": "^2.0.4",
    "lodash": "^4.17.20",
    "morgan": "^1.10.0",
    "ms": "^2.1.2",
    "mysql2": "^2.2.5",
    "nan": "^2.14.1",
    "nanoid": "^3.1.12",
    "nodemailer": "^6.4.11",
    "nunjucks": "^3.2.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.22.3",
    "sequelize-typescript": "^1.1.0",
    "slugify": "^1.4.5",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.32",
