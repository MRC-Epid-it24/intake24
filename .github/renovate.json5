{
  extends: [
    'config:recommended',
    'schedule:earlyMondays',
  ],
  labels: [
    'dependencies',
  ],
  pin: {
    enabled: false,
  },
  packageRules: [
    {
      matchPackageNames: [
        'execa',
        'formdata-node',
        'nanoid',
        'p-limit',
        'p-queue',
      ],
      matchUpdateTypes: [
        'major',
      ],
      enabled: false,
    },
    {
      matchPackageNames: [
        'typescript',
      ],
      matchUpdateTypes: [
        'major',
        'minor',
      ],
      groupName: 'typescript',
      groupSlug: 'typescript',
    },
    {
      matchUpdateTypes: [
        'minor',
        'patch',
      ],
      groupName: 'all non-major dependencies',
      groupSlug: 'all-minor-patch',
      matchPackageNames: [
        '*',
      ],
    },
  ],
  rangeStrategy: 'bump',
  ignoreDeps: [
    'node',
  ],
}
