# Node environment
NODE_ENV=test

# Application settings
APP_NAME=Intake24
APP_HOST=localhost
APP_PORT=3100
APP_SECRET=appSuperSecret

APP_URL_ADMIN=/admin
APP_URL_DOCS=/docs
APP_URL_SURVEY=/survey

# ACL settings
ACL_CACHE_ENABLED=false
ACL_CACHE_EXPIRES_IN=7d

# Cache settings
CACHE_REDIS_HOST=localhost
CACHE_REDIS_PORT=6379
CACHE_REDIS_PREFIX=it24:cache:

# Database

# System database connection info
#DB_SYSTEM_DRIVER=postgres
#DB_SYSTEM_HOST=localhost
