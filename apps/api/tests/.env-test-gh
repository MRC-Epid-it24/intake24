#DB_FOODS_PASSWORD=it24

# Filesystem
FS_LOGS=tests/tmp/logs
FS_DOWNLOADS=tests/tmp/downloads
FS_UPLOADS=tests/tmp/uploads
FS_IMAGES=tests/tmp/images

# Mail settings
MAIL_MAILER=smtp
MAIL_HOST=localhost
MAIL_PORT=25
MAIL_USERNAME=
MAIL_PASSWORD=
MAIL_SECURE=false
MAIL_IGNORE_TLS=true
MAIL_FROM_ADDRESS=example@domain.com
MAIL_FROM_NAME=$APP_NAME

# Queue settings
QUEUE_REDIS_HOST=localhost
QUEUE_REDIS_PORT=6379
QUEUE_WORKERS=3

# Security settings

# CORS settings
# comma-delimited origins
CORS_ORIGIN=

# JSON Web Token settings
JWT_ACCESS_SECRET=aJWTAccessSecret
JWT_ACCESS_LIFETIME=15m
JWT_REFRESH_SECRET=aJWTRefreshSecret
JWT_REFRESH_LIFETIME=1d
JWT_COOKIE_PATH=/api/auth
JWT_COOKIE_SAMESITE=
JWT_COOKIE_SECURE=false

# Multi-factor authentication
MFA_ENABLED=false
MFA_PROVIDER=duo

# DUO MFA provider settings
DUO_IKEY=
DUO_SKEY=
DUO_AKEY=duosecret
DUO_HOST=

# Session settings
SESSION_REDIS_HOST=localhost
SESSION_REDIS_PORT=6379
SESSION_REDIS_PREFIX=it24:session:

