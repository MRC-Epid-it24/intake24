  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach",
      "type": "pwa-node",
      "port": 5959,
      "request": "attach",
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true,
      "outFiles": ["${workspaceFolder:apps/api}/dist/**/*.js"],
      "sourceMapPathOverrides": {
        "webpack://@intake24/apps/api/./src/*": "${workspaceFolder:apps/api}/src/*"
      }
    }
  ]
}
