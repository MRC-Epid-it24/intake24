{
  "name": "@intake24/portal",
  "version": "0.1.0",
  "private": true,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/MRC-Epid-it24/intake24.git",
    "directory": "apps/portal"
  },
  "scripts": {
    "build": "pnpm build:back && pnpm build:front",
    "build:back": "pnpm build:back:clean && webpack --env NODE_ENV=production",
    "build:front": "mix --production",
    "build:back:clean": "rimraf dist",
    "build:front:clean": "rimraf public",
    "dev": "pnpm dev:back && dev build:front",
    "dev:back": "webpack --env NODE_ENV=development",
    "dev:front": "mix -- watch",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@popperjs/core": "^2.11.5",
    "bootstrap": "^5.1.3",
    "core-js": "^3.22.8",
    "dotenv": "^16.0.1",
    "dotenv-expand": "^8.0.3",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nunjucks": "^3.2.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/morgan": "^1.9.3",
    "@types/webpack-env": "1.17.0",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "laravel-mix": "^6.0.48",
    "nodemon": "^2.0.16",
    "nodemon-webpack-plugin": "^4.7.1",
    "postcss": "^8.4.14",
    "resolve-url-loader": "^5.0.0",
    "rimraf": "^3.0.2",
    "sass": "~1.32.13",
    "sass-loader": "~10.2.1",
    "ts-loader": "^9.3.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-node-externals": "^3.0.0",
    "webpackbar": "^5.0.2"
  },
  "bundledDependencies": [
    "@intake24/common",
    "@intake24/i18n",
    "@intake24/services"
  ]
}
